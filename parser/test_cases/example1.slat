# Define the IM function:
#                        v_asy, IM_asy, alpha
#detfn IM_FUNC hyperbolic 1221,   29.8,  62.2;
detfn IM_FUNC loglog    [0.01, 0.376775], [0.02, 0.155158], [0.04, 0.054048],
        [0.06, 0.027886], [0.08, 0.017241], [0.1, 0.011857],  [0.2, 0.003656],
	[0.3, 0.001628],  [0.4, 0.000794],  [0.5, 0.000405],  [0.6, 0.000214],
	[0.7, 0.000115],  [0.8, 0.000064],  [0.9, 0.000037],  [1.0, 0.000021],
	[1.1, 0.000013],  [1.2, 7.00E-06],  [1.3, 4.00E-06],  [1.4, 3.00E-06],
	[1.5, 1.80E-06],  [1.6, 1.00E-06],  [1.7, 7.00E-07],  [1.8, 4.00E-07],
	[1.9, 2.00E-07],  [2.0, 1.00E-07];

# Define the EDP-IM functions:
#                          a    b
#detfn MU_EDP powerlaw    0.1, 1.5;
#detfn SIGMA_EDP powerlaw 0.5, 0.0;

#probfn EDPIM_FUNC lognormal MU_EDP, SIGMA_EDP --mu mean_x --sd sd_ln_x;
importprobfn EDPIM_FUNC  /home/mag109/SLATv1.15_Public/example2_10storeybuilding/RB_EDP1.txt;

# Define relationships:
im IM_1 IM_FUNC;
collapse IM_1 0.9, 0.470 --mu mean_x --sd sd_ln_x;
edp EDP_1 IM_1 EDPIM_FUNC;

# Fragility, loss, component group:
fragfn FRAG_1 [0.0062, 0.4],
              [0.0230, 0.4],
              [0.0440, 0.4],
	      [0.0564, 0.4] 
        --mu mean_x --sd sd_ln_x;

lossfn LOSS_1 simple [0.03,0.4], 
       	      	     [0.08, 0.4],
		     [0.25, 0.4],
		     [1.00, 0.4]
              --mu mean_x --sd sd_ln_x;

compgroup COMPGROUP_1 EDP_1 FRAG_1 LOSS_1 1;

# Set up integration parameters:
integration maq 1E-6 1024;

# Set up recorders:
#recorder imrate IM_1 0.01:0.01:2.50 example1_im_rate.txt --new;
#recorder imrate IM_1 $(numpy.arange(0.01, 1.01, 0.01)) example1_im_rate.txt --new;
#set values $(numpy.arange(0.0, 1.01, 0.01));
set logvalues $(list(map(math.exp, math.log(0.01) + numpy.arange(0, 199, 1) * math.log(2.5/0.01)/198)));
set linvalues $(0.01 + numpy.arange(0, 199, 1) * (2.5 - 0.01)/198);
recorder imrate IM_1 $logvalues example1_im_rate.txt --new;
recorder collapse IM_1 $(0.01 + numpy.arange(0, 199, 1) * (2.5 - 0.01)/198) example1_collapse.txt --new;
recorder collrate IM_1 example1_collrate.txt --new;
recorder edpim EDP_1  $(0.01 + numpy.arange(0, 199, 1) * (2.5 - 0.01)/198) --cols mean_x, sd_ln_x, 0.16, 0.50, 0.84 example1_im_edp.txt --new;
recorder edprate EDP_1 $(list(map(math.exp, math.log(0.001) + numpy.arange(0, 149, 1) * math.log(0.15/0.001)/148)))  example1_edp_rate.txt --new;
recorder dsedp COMPGROUP_1 0.0:0.001:0.200  example1_ds_edp.txt --new;
recorder dsrate COMPGROUP_1;
recorder lossedp COMPGROUP_1 $(0.001 + numpy.arange(0, 149, 1) * (0.15 - 0.001)/148) --cols mean_x, sd_ln_x example1_loss_edp.txt --new;
recorder lossim COMPGROUP_1 $(0.01 + numpy.arange(0, 199, 1) * (2.5 - 0.01)/198)  --cols mean_x, sd_ln_x example1_loss_im.txt --new;
#recorder lossim COMPGROUP_1 0:100:1  --cols mean_x, sd_ln_x example1_loss_im.txt --new;
recorder annloss COMPGROUP_1 1.0:1.0:100.0 --lambda 0.06 example1_annual_loss.txt --new;
#recorder annloss COMPGROUP_1 0.0:1.0.0:100.0 --lambda 0.06;
recorder lossrate COMPGROUP_1 1E-4:4.8E-3:1.2 example1_loss_rate.txt --new;
analyze;


